{
  "name": "xrpl-minter",
  "version": "0.0.0-development",
  "description": "Mint NFTS on the XRP Ledger",
  "main": "./lib/src/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./lib/",
    "cm": "cz",
    "coverage": "codecov",
    "lint": "eslint ./src/**/* --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test:watch": "jest --watch",
    "test": "jest",
    "test:cover": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agoro-digital/xrpl-minter"
  },
  "license": "MIT",
  "author": {
    "name": "Luke Brobbin",
    "email": "lukebrobbs@users.noreply.github.com",
    "url": "https://github.com/lukebrobbs"
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "xrp",
    "nft",
    "typescript",
    "vscode",
    "jest",
    "husky",
    "commitizen",
    "semantic-release",
    "codecov"
  ],
  "bugs": {
    "url": "https://github.com/agoro-digital/xrpl-minter/issues"
  },
  "homepage": "https://github.com/agoro-digital/xrpl-minter#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^12.20.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.0.0",
    "codecov": "^3.8.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-formatter-pretty": "4.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "24.2.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-unicorn": "28.0.2",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "lint-staged": "^11.2.0",
    "semantic-release": "^18.0.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "dotenv": "^10.0.0",
    "loglevel": "^1.8.0",
    "xrpl": "^2.0.2"
  }
}
